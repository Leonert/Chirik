{
   "name": "frontend",
   "version": "0.1.0",
   "private": true,
   "dependencies": {
      "@emotion/react": "^11.10.6",
      "@emotion/styled": "^11.10.6",
      "@mui/icons-material": "^5.11.16",
      "@mui/material": "^5.12.1",
      "@reduxjs/toolkit": "^1.9.5",
      "@testing-library/jest-dom": "^5.16.5",
      "@testing-library/react": "^13.4.0",
      "@testing-library/user-event": "^13.5.0",
      "npm-run-all": "^4.1.5",
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "react-redux": "^8.0.5",
      "react-router-dom": "^6.10.0",
      "react-scripts": "5.0.1",
      "sass": "^1.62.0",
      "web-vitals": "^2.1.4"
   },
   "scripts": {
      "start-js": "react-scripts start",
      "start": "npm-run-all -p start-js watch-styles-sass",
      "build": "npm run stylelint-build && react-scripts build",
      "test": "react-scripts test",
      "eject": "react-scripts eject",
      "stylelint-fix-sass": "stylelint --fix 'src/**/*.scss'",
      "stylelint-dev": "stylelint 'src/**/*.scss' --cache --cache-location './.stylelintcache'; exit 0",
      "stylelint-build": "stylelint 'src/**/*.scss'",
      "watch-styles-sass": "npm-watch"
   },
   "watch": {
      "stylelint-dev": {
         "patterns": [
            "/src/**/*.scss"
         ],
         "extensions": "scss"
      }
   },
   "browserslist": {
      "production": [
         ">0.2%",
         "not dead",
         "not op_mini all"
      ],
      "development": [
         "last 1 chrome version",
         "last 1 firefox version",
         "last 1 safari version"
      ]
   },
   "devDependencies": {
      "@babel/core": "^7.21.4",
      "@babel/eslint-parser": "^7.21.3",
      "eslint": "^8.39.0",
      "npm-watch": "^0.11.0",
      "standard": "^17.0.0",
      "stylelint": "^15.5.0",
      "stylelint-config-standard": "^33.0.0",
      "stylelint-order": "^6.0.3"
   }
}